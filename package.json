{
  "name": "shopee-auto-checkin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --cache --cache-strategy content --ext .ts,.vue --fix src",
